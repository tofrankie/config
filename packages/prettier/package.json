{
  "name": "@tofrankie/prettier",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@10.28.0",
  "description": "Shared Prettier configuration for @tofrankie projects",
  "author": "Frankie <1426203851@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/tofrankie/config/tree/main/packages/prettier",
  "repository": "github:tofrankie/config",
  "bugs": "https://github.com/tofrankie/config/issues",
  "keywords": [
    "prettier",
    "prettier-config",
    "format",
    "formatter",
    "tofrankie"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "CHANGELOG.md",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "pnpm build",
    "publint": "publint"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0"
  },
  "devDependencies": {
    "prettier": "^3.8.1"
  }
}
