{
  "name": "@tofrankie/prettier",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@10.28.0",
  "description": "Shared Prettier configuration",
  "author": "Frankie <1426203851@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/tofrankie/config/tree/main/packages/prettier",
  "repository": "github:tofrankie/config",
  "bugs": "https://github.com/tofrankie/config/issues",
  "keywords": [
    "prettier",
    "prettier-config",
    "format",
    "formatter",
    "tofrankie"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./options/base": {
      "import": {
        "types": "./dist/options/base.d.mts",
        "default": "./dist/options/base.mjs"
      },
      "require": {
        "types": "./dist/options/base.d.cts",
        "default": "./dist/options/base.cjs"
      }
    },
    "./options/sort-html": {
      "import": {
        "types": "./dist/options/sort-html.d.mts",
        "default": "./dist/options/sort-html.mjs"
      },
      "require": {
        "types": "./dist/options/sort-html.d.cts",
        "default": "./dist/options/sort-html.cjs"
      }
    },
    "./options/sort-vue": {
      "import": {
        "types": "./dist/options/sort-vue.d.mts",
        "default": "./dist/options/sort-vue.mjs"
      },
      "require": {
        "types": "./dist/options/sort-vue.d.cts",
        "default": "./dist/options/sort-vue.cjs"
      }
    },
    "./options/sort-miniapp": {
      "import": {
        "types": "./dist/options/sort-miniapp.d.mts",
        "default": "./dist/options/sort-miniapp.mjs"
      },
      "require": {
        "types": "./dist/options/sort-miniapp.d.cts",
        "default": "./dist/options/sort-miniapp.cjs"
      }
    },
    "./options/sort-wechat-svg": {
      "import": {
        "types": "./dist/options/sort-wechat-svg.d.mts",
        "default": "./dist/options/sort-wechat-svg.mjs"
      },
      "require": {
        "types": "./dist/options/sort-wechat-svg.d.cts",
        "default": "./dist/options/sort-wechat-svg.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "CHANGELOG.md",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "pnpm build",
    "publint": "publint"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0"
  },
  "dependencies": {
    "prettier-plugin-organize-attributes": "^1.0.0"
  },
  "devDependencies": {
    "prettier": "^3.8.1"
  }
}
