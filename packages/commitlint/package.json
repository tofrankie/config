{
  "name": "@tofrankie/commitlint",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@10.28.0",
  "description": "Shared commitlint configuration",
  "author": "Frankie <1426203851@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/tofrankie/config/tree/main/packages/commitlint",
  "repository": "github:tofrankie/config",
  "bugs": "https://github.com/tofrankie/config/issues",
  "keywords": [
    "commitlint",
    "commitlint-config",
    "commit",
    "lint",
    "tofrankie"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "CHANGELOG.md",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "pnpm build",
    "publint": "publint"
  },
  "peerDependencies": {
    "@commitlint/cli": ">=20.0.0"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^20.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/types": "^20.4.0"
  }
}
