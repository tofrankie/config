{
  "name": "@tofrankie/eslint",
  "type": "module",
  "version": "0.0.8",
  "packageManager": "pnpm@10.28.0",
  "description": "Shared ESLint configuration",
  "author": "Frankie <1426203851@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/tofrankie/config/tree/main/packages/eslint",
  "repository": "github:tofrankie/config",
  "bugs": "https://github.com/tofrankie/config/issues",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "lint",
    "tofrankie"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "CHANGELOG.md",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "pnpm build",
    "publint": "publint"
  },
  "peerDependencies": {
    "@antfu/eslint-config": ">=7.0.0",
    "eslint": ">=9.0.0"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^2.12.4",
    "eslint-plugin-format": "^1.4.0",
    "eslint-plugin-jsdoc": "^62.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.3.0",
    "@types/lodash.merge": "^4.6.9",
    "eslint": "^9.39.2"
  }
}
